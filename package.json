  "description": "这是一个可视化蓝图（流程图）程序编辑器",
  "author": "梦欤 <1501076885@qq.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build": "node build/build.js",
    "build-electron-dev": "cross-env NODE_ENV=development webpack --progress --color --colors --watch --config build-electron/webpack.dev.conf.js",
    "build-electron-prod": "cross-env NODE_ENV=production node build-electron/build.js",
    "build-one": "cross-env NODE_ENV=production webpack --progress --color --colors --config build-electron/webpack.prod.conf.js",
    "run-electron-dev": "cross-env NODE_ENV=development electron ./dist/development/",
    "run-electron-prod": "cross-env NODE_ENV=production electron ./dist/production/",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder build",
    "pack-win32": "electron-builder build --ia32",
    "pack-win64": "electron-builder build --x64"
  },
